@model EMDViewModel

@{
    ViewData["Title"] = "Delete";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Xóa ?</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="/">Home</a></li>
                    <li class="breadcrumb-item"><a asp-action="Index">Danh Sách Đặt Cọc</a></li>
                    <li class="breadcrumb-item active">Xóa Đặt Cọc</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<!-- Main content -->
<section class="content">

    <!-- Default box -->
    <div class="card">
        <div class="card-header">
            <h3 class="card-title"><i class="fas fa-file text-info"></i> Xóa Đặt Cọc</h3>

            <div class="card-tools">
                <button type="button" class="btn btn-tool" data-card-widget="collapse" data-toggle="tooltip" title="Collapse">
                    <i class="fas fa-minus"></i>
                </button>
                <button type="button" class="btn btn-tool" data-card-widget="remove" data-toggle="tooltip" title="Remove">
                    <i class="fas fa-times"></i>
                </button>
            </div>
        </div>
        <div class="card-body">


            <form method="post" autocomplete="off">
                <div class="p-4 border rounded">

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.SGTCode" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.SGTCode" class="form-control" readonly />

                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.BatDau" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input type="text" asp-for="EMDTbl.BatDau" class="form-control maskInput" disabled />
                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.KetThuc" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input type="text" asp-for="EMDTbl.KetThuc" class="form-control maskInput" readonly />
                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.SLVeDatCoc" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.SLVeDatCoc" class="form-control" readonly />
                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.NgayDC" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input type="text" asp-for="EMDTbl.NgayDC" class="form-control maskInput" readonly />
                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.HetHan" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input type="text" asp-for="EMDTbl.HetHan" class="form-control maskInput" readonly />

                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.Number" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.Number" class="form-control" readonly />

                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.HangHK" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.HangHK" class="form-control" readonly />

                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.STDatCoc" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.STDatCoc" asp-format="{0:0,0}" class="form-control" readonly />

                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.HoanCoc" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input type="text" asp-for="EMDTbl.HoanCoc" class="form-control " readonly />

                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.LoaiBK" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.LoaiBK" class="form-control" readonly />

                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.SLVeDaXuat" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.SLVeDaXuat" class="form-control" readonly />

                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.TienXuatVe" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.TienXuatVe" class="form-control" readonly />

                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.TienPhat" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.TienPhat" class="form-control" readonly />

                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.NgayPhat" class="control-label "></label>
                        </div>
                        <div class="col-4">
                            <input type="text" value="" asp-for="EMDTbl.NgayPhat" class="form-control maskInput" readonly />

                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.SoCTPhat" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.SoCTPhat" class="form-control" readonly />

                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.LoaiTien" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.LoaiTien" class="form-control" readonly />

                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.TyGia" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.TyGia" class="form-control" readonly />

                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.GhiChu" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <textarea asp-for="EMDTbl.GhiChu" class="form-control" readonly></textarea>

                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.DongHoSo" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input type="text" value="" asp-for="EMDTbl.DongHoSo" class="form-control" readonly />

                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.NguoiNhap" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.NguoiNhap" class="form-control" readonly />

                        </div>


                        <div class="col-2">
                            <label asp-for="EMDTbl.PNR" class="control-label"></label>
                        </div>
                        <div class="col-4">
                            <input asp-for="EMDTbl.PNR" class="form-control" readonly />

                        </div>

                    </div>

                    <div class="form-group row">
                        <div class="col-2">
                            <label asp-for="EMDTbl.DienGiai" class="control-label"></label>
                        </div>
                        <div class="col-10">
                            <textarea asp-for="EMDTbl.DienGiai" class="form-control" rows="10" readonly></textarea>

                        </div>

                    </div>


                    <div class="form-group">
                        <button type="submit" class="btn btn-danger"><i class="fas fa-trash-alt"></i></button>
                        <a class="btn btn-success" asp-action="Index"><i class="fas fa-backward"></i></a>
                    </div>
                </div>
            </form>



        </div>
        <!-- /.card-body -->

    </div>
    <!-- /.card -->

</section>
<!-- /.content -->
